{
  "name": "@clanforge/worker",
  "version": "1.0.0",
  "description": "ClanForge Background Worker",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node src/main.ts",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "bull": "^4.11.0",
    "redis": "^4.6.0",
    "fluent-ffmpeg": "^2.1.2",
    "ffmpeg-static": "^5.2.0",
    "aws-sdk": "^2.1450.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.4",
    "twilio": "^4.15.0",
    "discord.js": "^14.13.0",
    "socket.io": "^4.7.0",
    "@clanforge/lib": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/bull": "^4.10.0",
    "@types/redis": "^4.0.11",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/multer": "^1.4.7",
    "@types/node-cron": "^3.0.7",
    "@types/nodemailer": "^6.4.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/*.test.ts"],
    "collectCoverageFrom": ["src/**/*.ts"]
  }
}
